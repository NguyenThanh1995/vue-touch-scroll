      "webpack": "^4.44.2",
      "webpack-cli": "^3.3.12"
   },
   "babel": {
      "presets": [
         [
            "@babel/preset-env",
            {
               "targets": {
                  "node": "current"
               }
            }
         ]
      ],
      "plugins": [
         "transform-es2015-modules-commonjs"
